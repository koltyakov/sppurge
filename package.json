{
  "name": "sppurge",
  "description": "Delete files from SharePoint document libraries using Node.js without hassles",
  "version": "2.2.0",
  "author": "Andrew Koltyakov <andrew.koltyakov@gmail.com>",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "scripts": {
    "build": "npm run tslint && npm run clean && npm run tsc",
    "tslint": "tslint -p .",
    "tsc": "tsc -p .",
    "clean": "rimraf ./dist",
    "test:prepare": "ts-node ./test/uploadStructure",
    "test": "npm run test:prepare && mocha --opts test/mocha.opts || ECHO.",
    "test:ci": "npm run test:prepare && mocha --opts test/mocha.opts --ci || ECHO.",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "bugs": {
    "url": "https://github.com/koltyakov/sppurge/issues"
  },
  "keywords": [
    "sharepoint",
    "delete",
    "files",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koltyakov/sppurge.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/koltyakov/sppurge#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "node-sp-auth": "^2.5.7",
    "sp-request": "^2.1.3",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/colors": "^1.2.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.2",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "spsave": "^3.1.6",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "snyk": true
}
